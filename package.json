{
  "name": "cesium-kit",
  "version": "0.1.7",
  "description": "Cesium utility components. RippleMarker | ViewerClick | CameraMoveEvent.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./styles/camera-control.css": "./dist/styles/camera-control.css"
  },
  "scripts": {
    "build": "tsx scripts/generate-exports.ts && tsup && cpy \"src/styles/**/*.css\" dist/styles --parents && cpy \"dist/**/*\" playground/src/cesium-kit --parents",
    "dev": "tsup src/index.ts --watch --dts --format esm,cjs",
    "clean": "rimraf dist",
    "pack": " npm version patch --no-git-tag-version && npm run build && npm pack",
    "prepublishOnly": "npm run build",
    "dev:play": "concurrently \"npm:build\" \"npm:play:start\"",
    "play:start": "npm --prefix playground run dev",
    "play:install": "npm --prefix playground i",
    "pb": "npm publish --access public",
    "upv": "npm version patch --no-git-tag-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leongaooo/cesium-kit.git"
  },
  "author": "GlobeX Contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leongaooo/cesium-kit/issues"
  },
  "homepage": "https://github.com/leongaooo/cesium-kit#readme",
  "keywords": [
    "cesium",
    "3d",
    "globe",
    "visualization",
    "ripple",
    "marker",
    "viewer",
    "click",
    "camera",
    "move",
    "event",
    "kit",
    "typescript"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cpy-cli": "^6.0.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "cesium": ">=1.103.0"
  },
  "peerDependenciesMeta": {
    "cesium": {
      "optional": false
    }
  }
}
